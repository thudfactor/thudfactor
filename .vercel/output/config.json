{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/blogroll/?$",
			"dest": "_render"
		},
		{
			"src": "^/books/?$",
			"dest": "_render"
		},
		{
			"src": "^/endmark/?$",
			"dest": "_render"
		},
		{
			"src": "^/feed\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/links/?$",
			"dest": "_render"
		},
		{
			"src": "^/patterns/kitchen-sink/?$",
			"dest": "_render"
		},
		{
			"src": "^/patterns/layout/?$",
			"dest": "_render"
		},
		{
			"src": "^/patterns/?$",
			"dest": "_render"
		},
		{
			"src": "^/playground/nature-of-code/randomness/1/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/zombies/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}