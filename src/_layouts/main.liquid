---
title: Default title
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, viewport-fit=cover">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/prism/9000.0.1/themes/prism.min.css" rel="stylesheet" />

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link
      rel="preconnect"
      href="https://fonts.gstatic.com"
      crossorigin>

    <link href="https://fonts.googleapis.com/css2?family=Azeret+Mono:ital,wght@0,600;1,400;1,600&family=Crete+Round:ital@0;1&family=Exo+2:ital,wght@0,100..900;1,100..900&family=Lora:ital,wght@0,400;0,500;0,600;1,400;1,500;1,600&family=Nunito+Sans:ital,opsz,wght@0,6..12,200;0,6..12,400;0,6..12,600;1,6..12,200;1,6..12,400;1,6..12,600&display=swap" rel="stylesheet"> <link rel="stylesheet" href="/styles/main.css" />

    <meta property="og:title" content="{{ title }}" />
    <meta property="og:description" content="{{ description }}" />
    <meta property="og:url" content="https://www.thudfactor.com{{ page.url }}" />
    {% if cover %}
      {% capture metaImg %}
        {% facebookImagePath cover.src %}{% endcapture %}
      <meta property="og:image" content="{{ metaImg | strip }}" />
      <meta name="twitter:image" content="{{ metaImg | strip }}" />
    {% endif %}

    <meta name="twitter:title" content="{{ title }}" />
    <meta name="twitter:description" content="{{ description }}" />
    <meta name="twitter:card" content="summary_large_image" />

    <title>{{ title }} | Thudfactor</title>
  </head>
  <body>
    <header>
      <div class="site-title">
        <a href="/">Thudfactor</a>
        <div class="site-statement">Heavy.</div>
      </div>
      <nav>
        <a href="/about">About</a>
        <a href="/now">Now</a>
        <a href="/archive">Archive</a>
        <a href="/feed.xml">RSS</a>
      </nav>
    </header>
    <div class="contentWrapper">
      <main>
        {{ content }}
      </main>
    </div>
    <footer>
      Copyright Â© 2023 J H Williams.
      <a rel="me" href="https://social.horrorhub.club/@thudfactor">Meet me on Mastodon</a>.
    </footer>
  </body>
</html>