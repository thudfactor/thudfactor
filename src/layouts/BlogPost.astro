---
import Page from "@components/PageItems/Page.astro";
import Content from "@components/PageItems/Content.astro";

const pageProps = Astro.props.frontmatter || Astro.props;
const { title, description, cover } = pageProps
---

<Page title={title} description={description} cover={cover}>
	<Content {...pageProps}>
		<slot />
	</Content>
</Page>
