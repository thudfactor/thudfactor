<div id="Target"></div>
<script>
	import p5 from "p5";

	const sketch = (p: p5) => {
		p.setup = () => {
			p.createCanvas(400, 400);
			p.background(128);
		};

		p.draw = () => {
			p.stroke("rebeccapurple");
			p.strokeWeight(100);
			p.point(200, 200);
		};
	};

	const target = document.getElementById("Target");

	if (target) {
		new p5(sketch, target);
	}
</script>
